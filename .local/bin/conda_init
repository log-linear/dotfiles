# conda init script auto-added to .bashrc/.zshrc, segmented out for
# portability's sake

# >>> conda initialize >>>
# !! Contents within this block are managed by 'conda init' !!
__conda_setup="$('~/.local/bin/miniconda3/bin/conda' 'shell.zsh' 'hook' 2> /dev/null)"
if [ $? -eq 0 ]; then
    eval "$__conda_setup"
else
    if [ -f "~/.local/bin/miniconda3/etc/profile.d/conda.sh" ]; then
        . "~/.local/bin/miniconda3/etc/profile.d/conda.sh"
    else
        export PATH="~/.local/bin/miniconda3/bin:$PATH"
    fi
fi
unset __conda_setup
# <<< conda initialize <<<
