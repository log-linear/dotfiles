#!/bin/sh

#
# fzf emoji selector
#

# cut -d ';' -f1 ~/.local/bin/emoji  
# printf "$(cat ~/.local/bin/emoji | fzf)"
# printf "$(cat ~/.local/bin/emoji | fzf)" | cut -d ' ' -f1 

emoji="$(cat ~/.local/bin/emoji | fzf)" 

echo $emoji | cut -d ' ' -f1 | wl-copy
notify-send "$(echo $emoji | cut -d ';' -f1) copied to clipboard"

